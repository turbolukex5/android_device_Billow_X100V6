/cache            ext4       /dev/block/mmcblk0p21
/system           ext4       /dev/block/mmcblk0p20
/system_image     emmc       /dev/block/mmcblk0p20
/data             ext4       /dev/block/mmcblk0p22   length=-16384
/boot             emmc       /dev/block/mmcblk0p8
/recovery         emmc       /dev/block/mmcblk0p9
/external_sd      vfat       /dev/block/mmcblk1p1 /dev/block/mmcblk1  flags=display="External SD";storage;wipeingui;removable
/usb-otg          vfat       /dev/block/sda1 /dev/block/sda           flags=display="USB OTG";storage;wipeingui;removable
